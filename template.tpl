___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Require the necessary APIs
const logToConsole = require('logToConsole');
const injectScript = require('injectScript');
const callInWindow = require('callInWindow');
const queryPermission = require('queryPermission');


const webhookURL = data.webhookURL; //get the webhook URL from the input field
const webhookData = data.webhookData; // get the data to send to the webhook as an array of objects
const scriptURL = 'https://cdn.jsdelivr.net/gh/mariusghitulescu/js-webhook@main/js-webhook.js'; //sends data to webhook

const webhookDataObject = {};
webhookData.forEach(el => {webhookDataObject[el.label] = el.value;}); // transforms the input data into a single object

const log = data.debug ? logToConsole : (() => {}); //only log to console if the box is checked 

log('webhookData =', webhookData);
log('webhookDataObject =', webhookDataObject);


// If the script loaded successfully, send data to webhook and log a success message
const onSuccess = () => {
  callInWindow('jsWebhook', webhookDataObject, webhookURL);
  log('Data sent to webhook successfully!');
  data.gtmOnSuccess();
};


// If the script fails to load, log a message and signal failure
const onFailure = () => {
  log('Failed to send data to webhook.');
  data.gtmOnFailure();
};



if (queryPermission('inject_script', scriptURL)) {
  injectScript(scriptURL, onSuccess, onFailure);
} else {
  log('Script load failed due to permissions mismatch.');
  data.gtmOnFailure();
}


